sonar.organization={{ github_organization }}
sonar.projectKey={{ github_organization }}_{{ directory_name }}
sonar.host.url=https://sonarcloud.io
sonar.sources=src/{{ package_name }}/
sonar.tests=tests/
sonar.links.homepage={{ repository_url }}
sonar.links.scm={{ repository }}
sonar.links.issue={{ repository_url }}/issues
sonar.links.ci={{ repository_url }}/actions
sonar.python.coverage.reportPaths=coverage.xml
sonar.python.xunit.reportPath=xunit-result.xml
sonar.python.pylint.reportPaths=pylint-report.txt
